<% provide :title, t(".all") %>
<div class="col-md-9 col-sm-12">
  <div class="row">
    <div class="col-md-10 form-group">
      <%= form_tag admin_users_path, method: :get, id: "search",
        class: "form-inline" do %>
        <%= render "admin/shared/search_form" %>
      <% end %>
    </div>
  </div>
  <table id="users-data-table" class="table table-hover">
    <thead>
      <tr>
        <th class="text-center"><%= t".number" %></th>
        <th class="text-center"><%= t".name" %></th>
        <th class="text-center"><%= t".email" %></th>
        <th class="text-center"><%= t".join_on" %></th>
        <th class="text-center"><%= t".option" %></th>
        <th class="text-center"><%= t".status" %></th>
      </tr>
    </thead>
    <tbody id="result_search">
      <%= render @users %>
    </tbody>
  </table>
  <%= will_paginate %>
</div>
