<% provide :title, @title %>
<%= render template: "users/show" %>
<div class="col-md-9">
  <div class="panel panel-default sidebar-menu">
    <div class="panel-heading">
      <h3 class="panel-title">
      <span><%= t".header_follow" %></span>
      </h3>
    </div>
    <div class="panel-body">
      <% if @users.any? %>
        <ul class="users follow">
          <%= render @users %>
        </ul>
      <% end %>
    </div>
  </div>
  <%= will_paginate %>
</div>
